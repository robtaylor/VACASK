        {
            "name": "Run sg13g2 test in console (Debug)",
            
            // Linux/mingw64
            "type": "cppdbg",
            "MIMode": "gdb",
            // "miDebuggerPath": "E:/mingw64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ], 

            // MSVC
            // "type": "cppvsdbg", 
            
            "request": "launch",
            "program": "${workspaceFolder}/../build.${workspaceRootFolderName}/Debug/simulator/vacask",
            "args": [ "-dp", "-df", "${workspaceFolder}/demo/ihp-sg13g2/mos.sim" ], 
            // "args": [ "-dp", "-df", "${workspaceFolder}/demo/ihp-sg13g2/hbt.sim" ], 
            // "args": [ "-dp", "-df", "${workspaceFolder}/demo/ihp-sg13g2/stdcell.sim" ], 
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/../build.${workspaceRootFolderName}/Debug/test/manual_run",
            "environment": [ 
                { "name": "PDK_ROOT", "value": "${workspaceFolder}/../IHP-Open-PDK" }, 
                { "name": "PDK", "value": "ihp-sg13g2" }, 
            ], 
            "externalConsole": false,
        }, 

        {
            "name": "Run test in console (Release)",
            "type": "cppdbg",
            "request": "launch",
